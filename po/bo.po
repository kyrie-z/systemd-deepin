msgstr "འཁྱེར་བདེ་ཞབས་ཞུ་གཟུགས་བརྙན་སྦྲེལ་བ་དང་བསྡམས་པ་ལ་བདེན་སྒྲིག་དགོས།"

#: src/portable/org.freedesktop.portable1.policy:34
msgid "Delete or modify portable service image"
msgstr "འཁྱེར་བདེ་ཞབས་ཞུ་གཟུགས་བརྙན་བསུབ/བཟོ་བཅོས།"

#: src/portable/org.freedesktop.portable1.policy:35
msgid "Authentication is required to delete or modify a portable service image."
msgstr "འཁྱེར་བདེ་ཞབས་ཞུ་གཟུགས་བརྙན་བསུབ་པ་དང་བཟོ་བཅོས་ལ་བདེན་སྒྲིག་དགོས།"

#: src/resolve/org.freedesktop.resolve1.policy:22
msgid "Register a DNS-SD service"
msgstr "DNS-SD ཞབས་ཞུ་ཐོ་འགོད།"

#: src/resolve/org.freedesktop.resolve1.policy:23
msgid "Authentication is required to register a DNS-SD service"
msgstr "DNS-SD ཞབས་ཞུ་ཐོ་འགོད་ལ་བདེན་སྒྲིག་དགོས།"

#: src/resolve/org.freedesktop.resolve1.policy:33
msgid "Unregister a DNS-SD service"
msgstr "DNS-SD ཞབས་ཞུ་ཐོ་འགོད་བསྡམས།"

#: src/resolve/org.freedesktop.resolve1.policy:34
msgid "Authentication is required to unregister a DNS-SD service"
msgstr "DNS-SD ཞབས་ཞུ་ཐོ་འགོད་བསྡམས་པར་བདེན་སྒྲིག་དགོས།"

#: src/resolve/org.freedesktop.resolve1.policy:132
msgid "Revert name resolution settings"
msgstr "མིང་མོས་འགྲེལ་བཤད (name resolution) སྒྲིག་སྟངས་ལོག་པ"

#: src/resolve/org.freedesktop.resolve1.policy:133
msgid "Authentication is required to reset name resolution settings."
msgstr "མིང་མོས་འགྲེལ་བཤད་སྒྲིག་སྟངས་བསྐྱར་སྒྲིག་ལ་བདེན་སྒྲིག་དགོས།"

#: src/timedate/org.freedesktop.timedate1.policy:22
msgid "Set system time"
msgstr "མ་ལག་དུས་ཚོད་སྒྲིག"

#: src/timedate/org.freedesktop.timedate1.policy:23
msgid "Authentication is required to set the system time."
msgstr "མ་ལག་དུས་ཚོད་སྒྲིག་པར་བདེན་སྒྲིག་དགོས།"

#: src/timedate/org.freedesktop.timedate1.policy:33
msgid "Set system timezone"
msgstr "མ་ལག་དུས་ཁུལ་སྒྲིག"

#: src/timedate/org.freedesktop.timedate1.policy:34
msgid "Authentication is required to set the system timezone."
msgstr "མ་ལག་དུས་ཁུལ་སྒྲིག་པར་བདེན་སྒྲིག་དགོས།"

#: src/timedate/org.freedesktop.timedate1.policy:43
msgid "Set RTC to local timezone or UTC"
msgstr "RTC རང་ས་དུས་ཁུལ་ཡང་ན UTC ལ་སྒྲིག"

#: src/timedate/org.freedesktop.timedate1.policy:44
msgid "Authentication is required to control whether the RTC stores the local or UTC time."
msgstr "RTC ལ་རང་ས་དུས་ཚོད་ཡང་ན UTC གསོག་སྲུང་བྱེད་དམ་མིན་ཚོད་འཛིིན་ལ་བདེན་སྒྲིག་དགོས།"

#: src/timedate/org.freedesktop.timedate1.policy:53
msgid "Turn network time synchronization on or off"
msgstr "དྲ་རྒྱའི་དུས་ཚོད་མཉམ་འཁོར་འགུལ/མཚམས་འཇོག"

#: src/timedate/org.freedesktop.timedate1.policy:54
msgid "Authentication is required to control whether network time synchronization shall be enabled."
msgstr "དྲ་རྒྱའི་དུས་ཚོད་མཉམ་འཁོར་འགུལ་བསྐྱོད་དམ་མིན་ཚོད་འཛིིན་ལ་བདེན་སྒྲིག་དགོས།"

#: src/core/dbus-unit.c:352
msgid "Authentication is required to start '$(unit)'."
msgstr "'$(unit)' འགོ་སློང་བར་བདེན་སྒྲིག་དགོས།"

#: src/core/dbus-unit.c:353
msgid "Authentication is required to stop '$(unit)'."
msgstr "'$(unit)' མཚམས་འཇོག་བྱེད་པར་བདེན་སྒྲིིག་དགོས།"

#: src/core/dbus-unit.c:354
msgid "Authentication is required to reload '$(unit)'."
msgstr "'$(unit)' བསྐྱར་འཇུག་བྱེད་པར་བདེན་སྒྲིིག་དགོས།"

#: src/core/dbus-unit.c:355 src/core/dbus-unit.c:356
msgid "Authentication is required to restart '$(unit)'."
msgstr "'$(unit)' བསྐྱར་འགོ་སློང་བར་བདེན་སྒྲིིག་དགོས།"

#: src/core/dbus-unit.c:553
msgid "Authentication is required to send a UNIX signal to the processes of '$(unit)'."
msgstr "'$(unit)' ཡི་ལས་རིམ་ཚོས་ UNIX མཚམས་མཚོན (signal) གཏོང་བར་བདེན་སྒྲིིག་དགོས།"

#: src/core/dbus-unit.c:583
msgid "Authentication is required to reset the \"failed\" state of '$(unit)'."
msgstr "'$(unit)' ཡི \"failed\" གནས་ཚུལ་བསྐྱར་སྒྲིིག་ལ་བདེན་སྒྲིིག་དགོས།"

#: src/core/dbus-unit.c:615
msgid "Authentication is required to set properties on '$(unit)'."
msgstr "'$(unit)' ལ་གཏོགས་པ་སྒྲིིག་པར་བདེན་སྒྲིིག་དགོས།"

#: src/core/dbus-unit.c:716
msgid "Authentication is required to delete files and directories associated with '$(unit)'."
msgstr "'$(unit)' དང་འབྲེལ་བའི་ཡིག་ཆ་དང་དཀར་ཆག་བསུབ་པར་བདེན་སྒྲིིག་དགོས།"

#: src/core/dbus-unit.c:764
msgid "Authentication is required to freeze or thaw the processes of '$(unit)' unit."
msgstr "'$(unit)' ཡི་ལས་རིམ་ཚོ་གཏོང་མཚམས (freeze)/ཚང་བསྐྱོད (thaw) བྱེད་པར་བདེན་སྒྲིིག་དགོས།"

#~ msgid "Authentication is required to halt the system while an application asked to inhibit it."
#~ msgstr "གློག་རྟགས་འཇུག་གིས་བཀག་ཞུས་དུས་མ་ལག་མཚམས་འཇོག་ལ་བདེན་སྒྲིག་དགོས།"

#~ msgid "Authentication is required to kill '$(unit)'."
#~ msgstr "'$(unit)' བསད་པར་བདེན་སྒྲིག་དགོས།"